config base_config 'base_config'
    option enabled '0'
    option portal_url 'http://thisnode.info/portal/'
    option db_path '/etc/pirania/db.csv'
    option hooks_path '/etc/pirania/hooks/'
    option uploadlimit '10000000'
    option downloadlimit '50000'
    option append_ipt_rules '0'  # if set to 1, iptables rules will be Appended instead of Inserted
    list whitelist_ipv4 '10.0.0.0/8'
    list whitelist_ipv6 '2a00::/8'
    list whitelist_ipv4 '172.16.0.0/12'
    list whitelist_ipv4 '192.168.0.0/16'
    list catch_interfaces 'br-lan'
    list catch_interfaces 'anygw'
